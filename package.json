{
  "name": "novella",
  "version": "0.0.0",
  "description": "Static site generator for web comics.",
  "repository": "git@github.com:cixzhang/Novella.git",
  "dependencies": {
    "lwip": "0.0.9"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "chai": "^2.2.0",
    "eslint": "^3.9.1",
    "eslint-plugin-vue": "^0.1.1",
    "markdown": "^0.5.0",
    "mocha": "^2.2.4",
    "natural-compare": "^1.4.0",
    "optionator": "^0.8.2",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.5",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-vue": "^2.2.21",
    "vue": "^2.0.5"
  },
  "main": "lib/novella.js",
  "bin": "lib/novella.js",
  "files": [
